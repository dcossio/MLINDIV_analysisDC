Combine_ROIs <- function(DF, Networklabels, netnumber, group) {
#' @description: purpose of this is to read in and extract the ROIs from schaefer atlas you choose.
#' The Networks labels is the dataframe with all of the labels
dataframe <-
as.data.frame(DF$subject_id) %>% `colnames<-`("subject_id")
if (netnumber == 7) {
if (group == "R") {
labels <-
str_remove(Networklabels$`Label Name`,
paste0(netnumber, "Networks_"))
for (label in labels) {
temp <-
DF %>% select(matches(label)) %>% rowMeans() %>% as.data.frame() %>% `colnames<-`(label)
dataframe <- cbind(dataframe, temp)
}
} else {
labels <-
c(
"LH_Vis",
"LH_SomMot",
"LH_DorsAttn",
"LH_SalVentAttn",
"LH_Limbic",
"LH_Cont",
"LH_Default",
"RH_Vis",
"RH_SomMot",
"RH_DorsAttn",
"RH_SalVentAttn",
"RH_Limbic",
"RH_Cont",
"RH_Default"
)
for (label in labels) {
temp <-
DF %>% select(matches(label)) %>% rowMeans() %>% as.data.frame() %>% `colnames<-`(label)
dataframe <- cbind(dataframe, temp)
}
}
} else{
if (group == "R") {
labels <-
str_remove(Networklabels$`Label Name`,
paste0(netnumber, "Networks_"))
for (label in labels) {
temp <-
DF %>% select(matches(label)) %>% rowMeans() %>% as.data.frame() %>% `colnames<-`(label)
dataframe <- cbind(dataframe, temp)
}
} else {
labels <-
c(
"LH_Vis",
"LH_SomMot",
"LH_DorsAttn",
"LH_SalVentAttn",
"LH_Limbic",
"LH_Cont",
"LH_Default",
"RH_Vis",
"RH_SomMot",
"RH_DorsAttn",
"RH_SalVentAttn",
"RH_Limbic",
"RH_Cont",
"RH_Default"
)
for (label in labels) {
temp <-
DF %>% select(matches(label)) %>% rowMeans() %>% as.data.frame() %>% `colnames<-`(label)
dataframe <- cbind(dataframe, temp)
}
}
}
}
